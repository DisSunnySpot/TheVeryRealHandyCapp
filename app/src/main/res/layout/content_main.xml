        app:layout_constraintTop_toTopOf="parent"></FrameLayout>

    <TextView

        android:id="@+id/textView7"
        android:layout_width="125dp"
        android:layout_height="76dp"
        android:layout_marginStart="128dp"
        android:layout_marginTop="404dp"
